<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

   <servlet>
    <servlet-name>HomePageServlet</servlet-name>
    <servlet-class>fk.sp.ListEasy.springs.controllers.HomePageServlet</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>SuccessPageServlet</servlet-name>
    <servlet-class>fk.sp.ListEasy.springs.controllers.SuccessPageServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>HomePageServlet</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>SuccessPageServlet</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>
  

  
</web-app>
